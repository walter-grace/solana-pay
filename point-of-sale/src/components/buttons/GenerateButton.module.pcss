.root {
    margin: 32px auto;
    padding: 12px 24px;
    border: 0;
    border-radius: 16px;
    display: block;
    font-size: 22px;
    line-height: 24px;
    font-weight: 500;
    cursor: pointer;
    background-color: Chartreuse;
    color:blue;
}

.root[disabled] {
    cursor: default;
    opacity: 0.9;
}

:global(html.light) .root {
    background: var(--theme-light-button-background);
    color: var(--theme-light-button-color);
}


